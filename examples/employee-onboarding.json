      "type": "task",
      "position": { "x": 400, "y": 180 },
      "data": {
        "id": "task_1",
        "type": "task",
        "label": "Prepare Workspace",
        "description": "Setup laptop, accounts, and workspace for new employee",
        "assignee": "it-support@company.com",
        "dueDate": "2024-01-14",
        "priority": "high",
        "customFields": {
          "laptopModel": "MacBook Pro M3",
          "monitors": "2x 27-inch",
          "accessories": "Keyboard, Mouse, Headset"
        }
      }
    },
    {
      "id": "automated_1",
      "type": "automated",
      "position": { "x": 400, "y": 310 },
      "data": {
        "id": "automated_1",
        "type": "automated",
        "label": "Create Email Account",
        "description": "Automatically provision company email and calendar",
        "actionId": "update_hrms",
        "actionLabel": "Update HRMS",
        "parameters": {
          "employeeId": "{{newHireId}}",
          "field": "status",
          "value": "active"
        }
      }
    },
    {
      "id": "automated_2",
      "type": "automated",
      "position": { "x": 400, "y": 440 },
      "data": {
        "id": "automated_2",
        "type": "automated",
        "label": "Send Welcome Email",
        "description": "Send welcome email with onboarding information",
        "actionId": "send_email",
        "actionLabel": "Send Email",
        "parameters": {
          "to": "{{newHireEmail}}",
          "subject": "Welcome to the Team!",
          "body": "Welcome! We're excited to have you join us. Your first day is {{startDate}}.",
          "cc": "hr@company.com"
        }
      }
    },
    {
      "id": "task_2",
      "type": "task",
      "position": { "x": 400, "y": 570 },
      "data": {
        "id": "task_2",
        "type": "task",
        "label": "Complete HR Paperwork",
        "description": "Fill out tax forms, benefits enrollment, and company policies",
        "assignee": "{{newHireEmail}}",
        "dueDate": "2024-01-15",
        "priority": "high",
        "customFields": {
          "forms": "W-4, I-9, Benefits Enrollment",
          "deadline": "First day of employment"
        }
      }
    },
    {
      "id": "automated_3",
      "type": "automated",
      "position": { "x": 400, "y": 700 },
      "data": {
        "id": "automated_3",
        "type": "automated",
        "label": "Generate Employment Contract",
        "description": "Generate and send employment contract for signature",
        "actionId": "generate_document",
        "actionLabel": "Generate Document",
        "parameters": {
          "template": "employment_contract",
          "recipient": "{{newHireName}}",
          "format": "pdf"
        }
      }
    },
    {
      "id": "approval_1",
      "type": "approval",
      "position": { "x": 400, "y": 830 },
      "data": {
        "id": "approval_1",
        "type": "approval",
        "label": "Manager Approval",
        "description": "Hiring manager reviews and approves onboarding completion",
        "approverRole": "Hiring Manager",
        "autoApproveThreshold": 95,
        "requiresComment": true,
        "escalationTimeout": 48
      }
    },
    {
      "id": "automated_4",
      "type": "automated",
      "position": { "x": 400, "y": 960 },
      "data": {
        "id": "automated_4",
        "type": "automated",
        "label": "Schedule Orientation",
        "description": "Schedule first-day orientation meeting",
        "actionId": "schedule_meeting",
        "actionLabel": "Schedule Meeting",
        "parameters": {
          "title": "New Hire Orientation",
          "attendees": "{{newHireEmail}}, hr@company.com, {{managerEmail}}",
          "duration": 120,
          "description": "Welcome orientation covering company culture, policies, and team introductions"
        }
      }
    },
    {
      "id": "automated_5",
      "type": "automated",
      "position": { "x": 400, "y": 1090 },
      "data": {
        "id": "automated_5",
        "type": "automated",
        "label": "Notify Team",
        "description": "Send Slack notification to team about new hire",
        "actionId": "send_slack_notification",
        "actionLabel": "Send Slack Message",
        "parameters": {
          "channel": "general",
          "message": "Please welcome {{newHireName}} to the team! They're joining as a {{position}} starting {{startDate}}.",
          "mention": "@channel"
        }
      }
    },
    {
      "id": "task_3",
      "type": "task",
      "position": { "x": 400, "y": 1220 },
      "data": {
        "id": "task_3",
        "type": "task",
        "label": "Assign Onboarding Buddy",
        "description": "Pair new hire with experienced team member for first 30 days",
        "assignee": "{{managerEmail}}",
        "priority": "medium",
        "customFields": {
          "buddyRole": "Senior Engineer",
          "duration": "30 days",
          "responsibilities": "Answer questions, provide guidance, schedule check-ins"
        }
      }
    },
    {
      "id": "end_1",
      "type": "end",
      "position": { "x": 400, "y": 1350 },
      "data": {
        "id": "end_1",
        "type": "end",
        "label": "Onboarding Complete",
        "description": "Employee successfully onboarded",
        "endMessage": "Congratulations! The onboarding process is complete. Welcome to the team!",
        "showSummary": true,
        "notifyUsers": [
          "hr@company.com",
          "{{managerEmail}}",
          "{{newHireEmail}}"
        ]
      }
    }
