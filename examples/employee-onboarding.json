        "id": "start_1",
        "type": "start",
        "label": "Start Onboarding",
        "description": "Initiate onboarding process for new hire",
        "metadata": {
          "department": "Engineering",
          "employeeType": "Full-time",
          "startDate": "2024-01-15",
          "location": "Remote"
        }
      }
    },
